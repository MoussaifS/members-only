
<%= render '/nav'%>
<h1> welcome <%= @user.name %> to your posts </h1>






<%@post.each do |post|%>
<div class="card">
  <h5 class="card-header">post id <%=post.id%></h5>
  <div class="card-body">
    <h5 class="card-title"><%= post.title%></h5>
    <p class="card-text"><%= post.body%></p>
    <a href="#" class="btn btn-primary"><%= post.user_id%></a>
  </div>
</div>
<%end%>



